extends layout

block content

	a(href="https://maps.google.co.uk/maps?q=" + flaskLocation)
		h1= "Photos taken near: " + flaskLocation

	each item in media
		.polaroid
			a(href=item.link)
				img(src=item.images.thumbnail.url)
			.details
			a(href="http://instagram.com/" + item.user.username)
				span="@" + item.user.username
